<form id="{{id}}">
    <h2>{{title}}</h2>
    {{#each elements}}
        {{#equals tag "button"}}
            <button name="{{name}}" type="{{type}}">{{value}}</button>
        {{else}}
            <{{tag}} name="{{name}}" placeholder="{{placeholder}}">{{value}}</{{tag}}>
        {{/equals}}
    {{/each}}
</form>
